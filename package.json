{
  "name": "qa.automationexercise-web.webdriverio",
  "version": "1.0.0",
  "type": "module",
  "description": "E2E Web Automation Testing with WebdriverIO",
  "main": "index.js",
  "scripts": {
    "test": "wdio run wdio.conf.js",
    "test:headless": "wdio run wdio.conf.js",
    "test:user": "wdio run wdio.conf.js --spec=./test/specs/user.spec.js",
    "test:product": "wdio run wdio.conf.js --spec=./test/specs/product.spec.js",
    "test:cart": "wdio run wdio.conf.js --spec=./test/specs/cart.spec.js",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:report": "npm run allure:generate && npm run allure:open"
  },
  "keywords": [
    "webdriverio",
    "e2e",
    "testing",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wdio/allure-reporter": "^8.40.0",
    "@wdio/cli": "^8.40.0",
    "@wdio/local-runner": "^8.40.0",
    "@wdio/mocha-framework": "^8.40.0",
    "@wdio/spec-reporter": "^8.40.0",
    "allure-commandline": "^2.29.0"
  }
}